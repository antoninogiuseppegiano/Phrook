{
  "https_port": 443,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  
  "ConnectionStrings": {
	"Default": ""
  },

  "Books": {
	  "PerPage": 30,
	  "Order": {
		  "By": "Title",
		  "Ascending": true,
		  "Allow": ["Title", "Rating", "Tag", "ReadingState"]
	  }
  }
}
