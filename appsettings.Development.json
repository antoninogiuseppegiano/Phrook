{
  "https_port": 5001,
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "ConnectionStrings": {
	"Default": ""
  },

  "Books": {
	  "PerPage": 10,
	  "Order": {
		  "By": "Title",
		  "Ascending": true,
		  "Allow": ["Title", "Rating", "Tag", "ReadingState"]
	  }
  }, 

  "Search": {
	"MaxResults": 100
  },

  "GoogleBooksApi": {
	"Url": "https://www.googleapis.com/books/v1/volumes"
  }
}
